public class {{model.name}} {{#if model.basepage.name}} extends {{model.basepage.name}}<{{model.name}}>{{/if}} {

{{#attributes}}
{{#if attribute.strategy}}
    @FindBy({{attribute.strategy}} = "{{{attribute.value}}}")
    private {{#equals type 'radio'}}List<WebElement>{{else}}WebElement{{/equals}} {{attribute.value}}
{{/if}}
{{/attributes}}

    public {{model.name}}(WebDriver driver) {
        super(driver)
    }

//Click elements
{{#attributes}}
    {{#if attribute.strategy}}
    {{model.name}} click{{proper attribute.value}}(){
        clickElement({{attribute.value}})
    }
    {{/if}}
{{/attributes}}


//SetText elements
{{#attributes}}
    {{#if attribute.strategy}}
    {{model.name}} set{{proper attribute.value}}Text(String s){
        sendKeys({{attribute.value}}, s)
    }
    {{/if}}
{{/attributes}}

}
